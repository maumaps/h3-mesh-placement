<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map>
<Map srs="+proj=utm +zone=38 +datum=WGS84 +units=m +no_defs" background-color="#16203a">
  <Style name="population-density">
    <Rule>
      <PolygonSymbolizer fill="[density_color]" fill-opacity="0.35"/>
      <LineSymbolizer stroke="#1b2b45" stroke-width="0.2" stroke-opacity="0.25"/>
    </Rule>
  </Style>

  <Style name="population-density-anim">
    <Rule>
      <PolygonSymbolizer fill="[density_color]" fill-opacity="0.13"/>
    </Rule>
  </Style>

  <Style name="elevation-empty">
    <Rule>
      <PolygonSymbolizer fill="[elev_color]" fill-opacity="0.7"/>
    </Rule>
  </Style>

  <Style name="roads-overlay">
    <Rule>
      <PolygonSymbolizer fill="#243a5a" fill-opacity="0.35"/>
    </Rule>
  </Style>

  <Style name="boundary-outline">
    <Rule>
      <LineSymbolizer stroke="#7aa6c2" stroke-width="1.1" stroke-opacity="0.85"/>
    </Rule>
  </Style>

  <Style name="country-border">
    <Rule>
      <LineSymbolizer stroke="#9fb7d1" stroke-width="1.6" stroke-opacity="0.9"/>
    </Rule>
  </Style>

  <Style name="tower-points">
    <Rule>
      <Filter>[source]='seed'</Filter>
      <MarkersSymbolizer width="9" height="9" fill="#f8fafc" fill-opacity="0.95" stroke="#0f172a" stroke-width="1.2"/>
    </Rule>
    <Rule>
      <Filter>[source] != 'seed'</Filter>
      <MarkersSymbolizer width="8" height="8" fill="#38bdf8" fill-opacity="0.9" stroke="#0c4a6e" stroke-width="1"/>
    </Rule>
    <Rule>
      <MarkersSymbolizer width="7" height="7" fill="#e2e8f0" fill-opacity="0.85" stroke="#0f172a" stroke-width="0.8"/>
    </Rule>
  </Style>

  <Style name="tower-active">
    <Rule>
      <MarkersSymbolizer width="32" height="32" fill="#fef08a" fill-opacity="0.22" stroke="#f59e0b" stroke-width="1.2"/>
      <MarkersSymbolizer width="20" height="20" fill="#fef08a" fill-opacity="0.4" stroke="#f59e0b" stroke-width="1.2"/>
      <MarkersSymbolizer width="12" height="12" fill="#f97316" fill-opacity="0.98" stroke="#7c2d12" stroke-width="1.2"/>
    </Rule>
  </Style>

