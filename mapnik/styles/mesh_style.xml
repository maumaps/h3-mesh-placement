  <Style name="frame-label-panel">
    <Rule>
      <PolygonSymbolizer fill="#0b1220" fill-opacity="0.82"/>
      <LineSymbolizer stroke="#3b4a63" stroke-width="1"/>
    </Rule>
  </Style>

  <Style name="frame-label">
    <Rule>
      <TextSymbolizer face-name="DejaVu Sans Book"
                      size="16"
                      fill="#f8fafc"
                      halo-fill="#0b1020"
                      halo-radius="2"
                      horizontal-alignment="left"
                      vertical-alignment="top"
                      placement="point"
                      allow-overlap="true">
        [label]
      </TextSymbolizer>
    </Rule>
  </Style>
